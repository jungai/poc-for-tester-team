{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "importsNotUsedAsValues": "error",
    "rootDir": "e2e",
    "outDir": "dist",
    "types": ["vitest/globals"]
  },
  "include": ["e2e/**/*.spec.ts"]
}
